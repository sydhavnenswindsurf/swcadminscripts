  "description": "",
  "main": "index.js",
  "scripts": {
    "start:udmeldelserapp": "set AppToStart=udmeldelserapp&& npm run start-local",
    "deploy:udmeldelserapp": "set AppToStart=udmeldelserapp && npm-run-all -l -p build typecheck -s deploy open-devurl",
    "start-devserver": "webpack-dev-server --env.project=%AppToStart% --env.mockApi",
    "open-devurl": "open-cli https://script.google.com/macros/s/AKfycbzZXTxJ32KdEDz6MXtfkA3dMv-o3j8UsbjOKrrOUcI/exec -- firefox",
    "start-local": "npm-run-all --parallel -l start-devserver \"typecheck --  --watch\"",
    "start-localx": "chokidar webpack.config.js \"%AppToStart%/**/*.*\" -i \"%AppToStart%/dist/**/*.*\" -t 500 -c \"npm-run-all --parallel --print-label start-devserver typecheck\" --initial",
    "start": "chokidar webpack.config.js \"%AppToStart%/**/*.*\" -i \"%AppToStart%/dist/**/*.*\" -t 500 -c \"npm-run-all --parallel --print-label build typecheck && cd %AppToStart% && npx clasp push  && open-cli https://script.google.com/macros/s/AKfycbzZXTxJ32KdEDz6MXtfkA3dMv-o3j8UsbjOKrrOUcI/exec -- firefox\" --initial",
    "build": "webpack --env.project=%AppToStart%",
    "typecheck": "tsc -p %AppToStart%",
    "deploy": "cd %AppToStart% && npx clasp push",
    "restore": "npm install --no-save",
    "watch": "gulp --watch",
    "default": "gulp --watch",
    "gapps-auth": "%SystemRoot%/system32/WindowsPowerShell/v1.0/powershell.exe -NoExit .\\authorize-gapps.ps1"
  },
